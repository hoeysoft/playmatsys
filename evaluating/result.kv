#:import ResultAccItem result.ResultAccItem

ResultAccItem:
    title: 'result'
    inneracc: inneracc
    InnerAccordion:
        id: inneracc

        litem: litem
        ditem: ditem
        ritem: ritem

        InnerAccItem:
            id: litem
            title: 'Left'
        InnerAccItem:
            id: ditem
            title: 'Draw'
        InnerAccItem:
            id: ritem
            title: 'Right'

<InnerAccItem>:
    graphslayout: graphslayout
    applybtn: applybtn

    PaddedBoxLayout:
        orientation: 'vertical'
        GraphsLayout:
            id: graphslayout
            rows: 2

        AnchorLayout:
            size_hint_y: 0.1
            padding: 30
            Button:
                id: applybtn
                size_hint_x: 0.3
                text: 'apply'


<DiffGraphLayout>:
    orientation: 'vertical'

    name: name
    graph: graph

    Graph:
        id: graph
    Label:
        id: name
        size_hint_y: 0.05
